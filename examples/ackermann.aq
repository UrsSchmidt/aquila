#!/usr/local/bin/aq

ackermann := \m : Integer, n : Integer ->
    if m = 0: n + 1
    elif m > 0 and n = 0: ackermann(m - 1, 1)
    elif m > 0 and n > 0: ackermann(m - 1, ackermann(m, n - 1))
    else: error('Illegal argument');

write int2str(ackermann(4, 0)); # 13
write int2str(ackermann(3, 4)); # 125
