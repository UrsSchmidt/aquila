#!/usr/local/bin/aq

For i From 2 To 100:
    numbers[i] := true;
    f := \i : Integer, j : Integer -> {
        If j < 2:
            Call error('Illegal argument');
        ElseIf i mod j = 0:
            numbers[i] := false;
        ElseIf j > 2:
            Call f(i, j - 1);
        EndIf
    };
    sq := sqrt(i);
    If sq >= 2:
        Call f(i, sq);
    EndIf
EndFor

Write dict2str(numbers);
