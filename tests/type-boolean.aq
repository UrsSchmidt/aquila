#!/usr/local/bin/aq

# literals
Write BoolToStr(false);
Write BoolToStr(true);

# operations
Write BoolToStr(not false);
Write BoolToStr(not true);

Write BoolToStr(false and false);
Write BoolToStr(false and true);
Write BoolToStr(true and false);
Write BoolToStr(true and true);

Write BoolToStr(false or false);
Write BoolToStr(false or true);
Write BoolToStr(true or false);
Write BoolToStr(true or true);

Write BoolToStr(false xor false);
Write BoolToStr(false xor true);
Write BoolToStr(true xor false);
Write BoolToStr(true xor true);

# functions
Write BoolToStr(StrToBool('false'));
Write BoolToStr(StrToBool('true'));
