#!/usr/local/bin/aq

# literals
Write bool2str(false);
Write bool2str(true);

# operations
Write bool2str(not false);
Write bool2str(not true);

Write bool2str(false and false);
Write bool2str(false and true);
Write bool2str(true and false);
Write bool2str(true and true);

Write bool2str(false or false);
Write bool2str(false or true);
Write bool2str(true or false);
Write bool2str(true or true);

Write bool2str(false xor false);
Write bool2str(false xor true);
Write bool2str(true xor false);
Write bool2str(true xor true);

# functions
Write bool2str(str2bool('false'));
Write bool2str(str2bool('true'));
