#!/usr/local/bin/aq

# literals
Write dict2str({});
Write dict2str({ 1, 2, 3 });
Write dict2str({ a: 1, b: 2, c: 3 });
Write dict2str({ ['a']: 1, ['b']: 2, ['c']: 3 });

# operations
a := { a: 1, b: 2, c: 3 };
Write int2str(a.b);
b := { 1, 2, 3 };
Write int2str(b[1]);
Write bool2str(2 : { 1, 2, 3 });
Write bool2str(4 : { 1, 2, 3 });

# functions
Write int2str(size({}));
Write int2str(size({ 1, 2, 3 }));

# dict2str
d := { a: \x, y -> x * y };
sd := dict2str(d);
Write sd;
dsd := str2dict(sd);
sdsd := dict2str(dsd);
Write sdsd;
