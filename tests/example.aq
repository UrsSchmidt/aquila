#!/usr/local/bin/aq

abc.def := 2;
Write int2str(abc.def);

a := {
    mykey: 'myvalue',
    ['mystringkey']: 'myvalue2',
    [2 + 3 * 4]: 42 * 3,
};
Write int2str(size(a));
Write dict2str(a);
Write a.mykey;
Write bool2str('myvalue' : a);

Remove a['mystringkey'];
Call foreach(a, \k, v -> {
    If IsInteger(v):
        Write k & ': ' & int2str(v);
    ElseIf IsString(v):
        Write k & ': ' & v;
    Else:
        Write k & ': ?';
    EndIf
});

# array example
array := { 5, 4, 3, 2, 1 };
array[4] := 42;
Remove array[2];
Write dict2str(array);
Call foreach(array, \k, v -> {
    Write int2str(v);
});
