#!/usr/local/bin/aq

abc.def := 2;
Write IntToStr(abc.def);

a := {
    mykey: 'myvalue',
    ['mystringkey']: 'myvalue2',
    [2 + 3 * 4]: 42 * 3,
};
Write IntToStr(size(a));
Write DictToStr(a);
Write a.mykey;
Write BoolToStr('myvalue' : a);

Remove a['mystringkey'];
Call foreach(a, \k, v -> {
    If IsInteger(v):
        Write k & ': ' & IntToStr(v);
    ElseIf IsString(v):
        Write k & ': ' & v;
    Else:
        Write k & ': ?';
    EndIf
});

# array example
array := { 5, 4, 3, 2, 1 };
array[4] := 42;
Remove array[2];
Write DictToStr(array);
Call foreach(array, \k, v -> {
    Write IntToStr(v);
});
