#!/usr/local/bin/aq

Run '../prelude/IntegerFunctions.aq';

# literals
Write IntToStr(000);
Write IntToStr(12345);
Write IntToStr(0b10101010);
Write IntToStr(0o755);
Write IntToStr(0xabcd);

# operations
Write IntToStr(1 + 2 * 3);
Write IntToStr(+72);
Write IntToStr(-72);
Write IntToStr(2 + 3);
Write IntToStr(2 - 3);
Write IntToStr(2 * 3);
Write IntToStr(3 / 2);
Write IntToStr(4 mod 3);
Write IntToStr(-4 mod 3);
#Write IntToStr(4 mod -3);
#Write IntToStr(-4 mod -3);
Write IntToStr(4 rem 3);
Write IntToStr(-4 rem 3);
Write IntToStr(4 rem -3);
Write IntToStr(-4 rem -3);
Write BoolToStr(2 = 2);
Write BoolToStr(2 = 3);
Write BoolToStr(2 <> 2);
Write BoolToStr(2 <> 3);
Write IntToStr(1!);
Write IntToStr(6!);
Write IntToStr(|0|);
Write IntToStr(|72|);
Write IntToStr(|-72|);

# functions
Write IntToStr(StrToInt('00'));
Write IntToStr(StrToInt('42'));
Write IntToStr(gcd(8, 4));
Write IntToStr(pow(0, 0));
Write IntToStr(pow(0, 1));
Write IntToStr(pow(1, 0));
Write IntToStr(pow(2, 3));
Write IntToStr(sgn(0));
Write IntToStr(sgn(72));
Write IntToStr(sgn(-72));
Write IntToStr(sqrt(0));
Write IntToStr(sqrt(49));

# prelude functions
Write IntToStr(binomial(2, 2));
Write IntToStr(lcm(5, 2));
