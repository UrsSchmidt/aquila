#!/usr/local/bin/aq

Run '../prelude/integerfunctions.aq';

# literals
Write int2str(000);
Write int2str(12345);
Write int2str(0b10101010);
Write int2str(0o755);
Write int2str(0xabcd);

# operations
Write int2str(1 + 2 * 3);
Write int2str(+72);
Write int2str(-72);
Write int2str(2 + 3);
Write int2str(2 - 3);
Write int2str(2 * 3);
Write int2str(3 / 2);
Write int2str(4 mod 3);
Write int2str(-4 mod 3);
#Write int2str(4 mod -3);
#Write int2str(-4 mod -3);
Write int2str(4 rem 3);
Write int2str(-4 rem 3);
Write int2str(4 rem -3);
Write int2str(-4 rem -3);
Write bool2str(2 = 2);
Write bool2str(2 = 3);
Write bool2str(2 <> 2);
Write bool2str(2 <> 3);
Write int2str(1!);
Write int2str(6!);
Write int2str(|0|);
Write int2str(|72|);
Write int2str(|-72|);

# functions
Write int2str(str2int('00'));
Write int2str(str2int('42'));
Write int2str(gcd(8, 4));
Write int2str(pow(0, 0));
Write int2str(pow(0, 1));
Write int2str(pow(1, 0));
Write int2str(pow(2, 3));
Write int2str(sgn(0));
Write int2str(sgn(72));
Write int2str(sgn(-72));
Write int2str(sqrt(0));
Write int2str(sqrt(49));

# prelude functions
Write int2str(binomial(2, 2));
Write int2str(lcm(5, 2));
